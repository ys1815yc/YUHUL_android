    <LinearLayout
        android:id="@+id/buttonLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/resultLinearLayout"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:orientation="horizontal">

        <ToggleButton
            android:id="@+id/trynotifytogglebtn"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@drawable/togglebutton_selector"
            android:text=""
            android:textOff="@string/try_off"
            android:textOn="@string/try_on"
            android:textSize="12sp" />

        <ToggleButton
            android:id="@+id/recToggleButton"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@drawable/togglebutton_selector"
            android:text=""
            android:textOff="@string/rec_off"
            android:textOn="@string/rec_on"
            android:textSize="12sp" />


        <Button
            android:id="@+id/notifybtn"
            style="?android:attr/buttonBarButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/block_g2"
            android:text="@string/gatt_services_notify"
            android:visibility="gone" />

        <Button
            android:id="@+id/calculateOnline"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@drawable/block_g2"
            android:text="@string/calculate"
            android:textSize="12sp"
            android:visibility="visible" />

        <Button
            android:id="@+id/saveBtn"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:background="@drawable/block_g2"
            android:text="@string/title_save"
            android:textSize="12sp" />

